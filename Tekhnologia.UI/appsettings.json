{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../Tekhnologia/Tekhnologia.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api-route": {
        "ClusterId": "api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api-cluster": {
        "Destinations": {
            "api-destination": {
            "Address": "http://localhost:7137"
          }
        }
      }
    }
  }
}
